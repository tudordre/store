<h1>New Article</h1>
<%= link_to "New Article", new_article_path %>
<br/>
<%= link_to "All Articles", articles_path %>
<%= form_with model: @article do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>